{
  "name": "backend-v2",
  "version": "1.0.0",
  "description": "Next-generation subscription manager backend with TypeScript, Fastify, and PostgreSQL",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:workers": "tsx watch src/workers.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:workers": "node dist/workers.js",
    "migrate": "tsx src/db/migrate.ts up",
    "migrate:status": "tsx src/db/migrate.ts status",
    "migrate:reset": "tsx src/db/migrate.ts reset",
    "migrate:json": "tsx src/db/seeds/migrate-from-json.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "subscription",
    "manager",
    "fastify",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.7.0",
    "@fastify/swagger-ui": "^5.2.5",
    "@fastify/websocket": "^11.2.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.69.3",
    "dotenv": "^17.3.1",
    "fastify": "^5.7.4",
    "fastify-plugin": "^5.1.0",
    "googleapis": "^171.4.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.22.0",
    "pg": "^8.18.0",
    "redis": "^5.11.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^4.0.18",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
